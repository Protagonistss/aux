{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "baseUrl": ".",
    "skipLibCheck": true,
    "strictNullChecks": true
  },
  "include": ["./*.ts", "./*.vue"],
  "exclude": ["dist", "node_modules"]
}
